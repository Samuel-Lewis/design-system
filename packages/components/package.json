    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@samuel-lewis/tsconfig": "*",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.27.0",
    "eslint-config-samuel-lewis": "*",
    "tsup": "^5.12.9",
    "typescript": "^4.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.8.0",
    "@mantine/hooks": "^5.8.0",
    "@tabler/icons": "^1.111.0",
    "react": "^18.1.0"
  }
