  "devDependencies": {
    "@samuel-lewis/tsconfig": "*",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.37.0",
    "eslint-config-samuel-lewis": "*",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.4",
    "@mantine/hooks": "^6.0.4",
    "@tabler/icons-react": "^2.11.0",
    "react": "^18.2.0"
  }
